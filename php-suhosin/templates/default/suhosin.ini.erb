extension=suhosin.so

[suhosin]
suhosin.get.max_value_length = <%= node["suhosin"]["get"]["max_value_length"] %>
suhosin.post.max_vars = <%= node["suhosin"]["post"]["max_vars"] %>
suhosin.request.max_vars = <%= node["suhosin"]["request"]["max_vars"] %>
suhosin.session.encrypt = <%= node["suhosin"]["session"]["encrypt"] %>
suhosin.executor.include.whitelist = phar
